{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": false,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "noImplicitReturns": true,
    "noEmitOnError": true,
    "lib": ["es2017", "dom"],
    "jsx": "react"
  },
  "exclude": [
    "node_modules"
  ]
}
